<h1 class="text-2xl">Users Listing</h1>
</br>
<table class="w-full">
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Company</th>
      <th colspan="3">Action</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.full_name %></td>
        <td><%= user.email %></td>
        <td><%= user.company.name %></td>
        <td><%= link_to "Show", admin_users_path(user) %></td>
      </tr>
    <% end %>
  </tbody>
</table>